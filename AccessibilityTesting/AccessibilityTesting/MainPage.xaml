<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AccessibilityTesting.MainPage">
    <ContentPage.Resources>
        <ResourceDictionary>
            <Style TargetType="Button">
                <Setter Property="FontSize" Value="32"/>
            </Style>
        </ResourceDictionary>
    </ContentPage.Resources>
    <StackLayout>
        <Button Text="Go to Button Page" x:Name="ButtonPage" Clicked="ButtonPage_Clicked"/>
        <Button Text="Go to Label Page" x:Name="LabelPage" Clicked="LabelPage_Clicked"/>
        <Button Text="Go to Frame Page" x:Name="FramePage" Clicked="FramePage_Clicked"/>
        <Button Text="Go to Listview Page" x:Name="ListviewPage" Clicked="ListviewPage_Clicked"/>
        <Button Text="Go to Entry Page" x:Name="EntryPage" Clicked="EntryPage_Clicked"/>
    </StackLayout>
</ContentPage>
